{
  "name": "forma-embedded-view-sdk",
  "author": "Autodesk Inc.",
  "version": "0.16.1",
  "license": "Apache-2.0",
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "rm -rf dist/ && tsc",
    "build:package": "yarn build && yarn pack",
    "prepare": "husky install",
    "prepublishOnly": "yarn build",
    "lint": "eslint . --max-warnings 0",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "./auto",
    "./dist"
  ],
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./auto": {
      "types": "./dist/auto.d.ts",
      "import": "./dist/auto.js"
    },
    "./library": {
      "types": "./dist/library.d.ts",
      "import": "./dist/library.js"
    },
    "./integrate-elements": {
      "types": "./dist/integrate.d.ts",
      "import": "./dist/integrate.js"
    },
    "./extensions": {
      "types": "./dist/extensions.d.ts",
      "import": "./dist/extensions.js"
    },
    "./generators": {
      "types": "./dist/generators.d.ts",
      "import": "./dist/generators.js"
    },
    "./terrain": {
      "types": "./dist/terrain.d.ts",
      "import": "./dist/terrain.js"
    },
    "./sun": {
      "types": "./dist/sun.d.ts",
      "import": "./dist/sun.js"
    },
    "./project": {
      "types": "./dist/project.d.ts",
      "import": "./dist/project.js"
    },
    "./proposal": {
      "types": "./dist/proposal.d.ts",
      "import": "./dist/proposal.js"
    },
    "./geometry": {
      "types": "./dist/geometry.d.ts",
      "import": "./dist/geometry.js"
    },
    "./camera": {
      "types": "./dist/camera.d.ts",
      "import": "./dist/camera.js"
    },
    "./render": {
      "types": "./dist/render.d.ts",
      "import": "./dist/render.js"
    },
    "./scene/selection": {
      "types": "./dist/selection.d.ts",
      "import": "./dist/selection.js"
    },
    "./analysis": {
      "types": "./dist/analysis.d.ts",
      "import": "./dist/analysis.js"
    },
    "./areaMetrics": {
      "types": "./dist/areaMetrics.d.ts",
      "import": "./dist/areaMetrics.js"
    },
    "./experimental": {
      "types": "./dist/experimental.d.ts",
      "import": "./dist/experimental.js"
    }
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "dependencies": {
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@semantic-release/exec": "^6.0.3",
    "@tsconfig/node16": "^16.1.1",
    "@tsconfig/strictest": "^2.0.2",
    "@types/estree": "^1.0.0",
    "@types/geojson": "^7946.0.0",
    "@types/node": "^18",
    "@types/uuid": "^9.0.3",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "ava": "^5.0.1",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-ava": "^14.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "semantic-release": "^21.1.1",
    "typescript": "^5.2.2"
  },
  "resolutions": {
    "*/eslint-plugin-compat/semver": "^7.5.4"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-organize-imports"
    ],
    "semi": false
  }
}
